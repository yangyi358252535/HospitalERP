		searchStudent();
		return "toList";
	}

	@Action(value = "toReload")
	public String toReload() {
		searchStudent();
		return "toList";
	}

	public String getFlagString() {
		return flagString;
	}

	public void setFlagString(String flagString) {
		this.flagString = flagString;
	}

	public String getInformation() {
